<% layout('/layouts/boilerplate.ejs') %>

    <div class="alert alert-danger my-5" role="alert">
        <h4 class="alert-heading">Error - Something Went Wrong</h4>
        <p>
            <%= err.message %> - Error Status: <%= err.status %>
        </p>
        <hr>
        <p class="mb-0">
            <%= err.stack %>
        </p>
    </div>